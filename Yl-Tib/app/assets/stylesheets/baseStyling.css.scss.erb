* {
    margin: 0px;
    padding: 0px;
}

body {
    background-image: -ms-linear-gradient(top left, #96A8A0 0%, #BFBB5A 100%);
    background-image: -moz-linear-gradient(top left, #96A8A0 0%, #BFBB5A 100%);
    background-image: -o-linear-gradient(top left, #96A8A0 0%, #BFBB5A 100%);
    background-image: -webkit-gradient(linear, left top, right bottom, color-stop(0, #96A8A0), color-stop(1, #BFBB5A));
    background-image: -webkit-linear-gradient(top left, #96A8A0 0%, #BFBB5A 100%);
    background-image: linear-gradient(to bottom right, #96A8A0 0%, #BFBB5A 100%);
    background-size: cover;
    background-repeat: no-repeat;
    background-attachment: fixed;
}

#BodyDiv {
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    -o-box-sizing: border-box;
    box-sizing: border-box;
    background-color: rgba(0, 0, 0, 0.4);
    padding: 6px;
    width: 100%;
    /*margin-right: 10%;
    margin-left: 10%;*/
    border: solid 2px #adb3a8;
    height: inherit;
    float: left;
    /*border: solid 2px #b8860b;*/
}

#MainDiv {
    position: relative;
    margin-left: 3%;
    margin-right: 3%;
    width: 94%;
    /*height: 200px;*/
    margin-top: 15px;
    float: left;
}

.DivTitle {
    font-family: 'Rammetto One', cursive;
    font-size: 8px;
    color: #d4dace;
    text-align: center;
    background-color: rgba(0, 0, 0, 0.4);
    border-bottom: 2px solid white;
    padding: 2px 0px 2px 0px;
    margin-bottom: 10px;
    padding-top: 5px;
}

.BoxedDiv {
    color: #d4dace;
    text-align: center;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    -o-box-sizing: border-box;
    box-sizing: border-box;
    background-color: rgba(0, 0, 0, 0.3);
    padding: 15px;
    width: 100%;
    margin-top: 5px;
    margin-bottom: 10px;
    height: inherit;
    border: solid 2px #adb3a8;
}