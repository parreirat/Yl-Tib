#FooterDiv {
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    -o-box-sizing: border-box;
    box-sizing: border-box;
    position: relative;
    padding-top: 2px;
    padding-bottom: 2px;
    margin-top: 20px;
    margin-left: 2%;
    margin-right: 2%;
    width: 96%;
    float: left;
    /*clear: both;*/
    background-color: rgba(0, 0, 0, 0.5);
    border: solid 2px #b8860b;
}

#FooterText {
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    -o-box-sizing: border-box;
    box-sizing: border-box;
    padding-top: 8px;
    padding-left: 10px;
    padding-right: 10px;
    padding-bottom: 8px;
    font-size: 90%;
    font-family: sans-serif;
    color: #d4dace;
    float: left;
}
